<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from LoginService WSDL service -->

  <service name="Login" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://project"/>
      <parameter name="wsdlServiceElement" value="LoginService"/>
      <parameter name="schemaQualified" value="http://project"/>
      <parameter name="wsdlServicePort" value="Login"/>
      <parameter name="className" value="project.LoginSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Login"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="login_details" qname="operNS:login_details" xmlns:operNS="http://project" returnQName="retNS:login_detailsReturn" xmlns:retNS="http://project" returnType="rtns:User_Details" xmlns:rtns="http://project" soapAction="" >
        <parameter qname="pns:email" xmlns:pns="http://project" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:pass" xmlns:pns="http://project" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="login_details"/>

      <typeMapping
        xmlns:ns="http://project"
        qname="ns:User_Details"
        type="java:project.User_Details"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
